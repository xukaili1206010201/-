  <div class="user-info">
      <p>{{formData.nickname}}</p>
      <p><i class="glyphicon glyphicon-phone"></i>{{formData.mobile}}</p>
  </div>
  <div class="pull-right"><i class="fa fa-angle-right"></i></div>
</div>


