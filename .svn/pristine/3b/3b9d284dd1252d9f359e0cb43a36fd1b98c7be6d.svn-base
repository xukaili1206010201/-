* {
  margin: 0;
  padding: 0;
}

.line-wrapper {
  width: 100%;
  overflow: hidden;
  border-bottom: 1px solid #aaa;
}

.line-scroll-wrapper {
  /*white-space: nowrap;*/
  clear: both;
}

.line-btn-delete {
  float: left;
  width: 80px;
  display: none;
}

.line-btn-delete button {
  width: 100%;
  height: 100%;
  background: red;
  border: none;
  /*font-size: 24px;*/
  font-family: 'Microsoft Yahei';
  color: #fff;
}

.line-normal-wrapper {
  display: inline-block;
  width: 150%;
  float: left;
  margin-top: 10px;
  margin-bottom: 10px;
}

.line-normal-community-name {
  color: #4e4e4e;
  margin-left: 10px;
  margin-bottom: 5px;
  font-size: 14px;
}

.line-normal-city-name{
  font-size: 12px;
  margin-left: 10px;
}

.line-normal-floor{
  font-size: 12px;
  float: right;
  margin-right: 10px;
}
