<nav class="navbar navbar-fixed-top navigationbar">
  <div class="navLeft">
    <!-- 返回按钮-->
    <img ng-click="backMycommunity()" class="back-button" src="images/back_button.png">
  </div>
  <!-- 搜索框组-->
  <div class="navText">我的小区设置</div>

</nav>


<div class="myhouse-set-content">
  <div class="current-city" ng-click="selectCity()">
    <p class="community_edit_font myhouse-set-cityname">{{cityName}}</p>
    <span style="display: none;">{{cityId}}</span>
  </div>

  <form ng-submit="submitCommunityInfo()" name="communityset" class="info-input-block">

    <div class="text-input-block">
      <div class="input-field-block">
        <input ng-model="userName" type="text" class="form-control community_edit_font" placeholder="姓名*">
      </div>
    </div>

    <div class="text-input-block">
      <div class="input-field-block">
        <input ng-model="telphone" name="mobile" type="text" maxlength="11" class="form-control community_edit_font"
               placeholder="电话号码*">
      </div>
    </div>

    <div class="text-input-block" ng-click="selectCommunity()">
      <p class="text community_edit_font myhouse-set-cityname">{{communityName}}</p>
    </div>

    <div class="text-input-block">
      <div class="input-field-block">
        <input ng-model="floor" type="text" class="form-control community_edit_font" placeholder="楼房号">
      </div>
    </div>


    <div class="navbar navbar-default navbar-fixed-bottom text-center buttonOne">
      <button type="submit" class="btn btn-success btnRadius" id="submit-button">提交</button>
    </div>

  </form>

</div>





