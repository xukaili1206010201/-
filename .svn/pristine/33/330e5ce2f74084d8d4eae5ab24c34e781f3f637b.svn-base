.second-image {
  border-left: 1px solid #e5e5e5;
  border-right: 1px solid #e5e5e5;
}
.comservice-empty{
  height:49px;
}
.cg-busy-backdrop{
  height:600px;
  margin-top:-230px;
}

.rules{
  width:100%;
  height:8px;
  background:#F0EFF5;
