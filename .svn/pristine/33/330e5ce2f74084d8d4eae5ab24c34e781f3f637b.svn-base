.comservice-content {
  /*position: absolute;*/
}

.owl-carousel .adImage {
  width: 100%;
  height: 150px;
}

.comservice-user-info {
  width: 100%;
  padding-top: 10px;
  padding-bottom: 7px;
  /*margin-bottom: 10px;*/
  overflow: hidden;
  background: white;
}

.comservice-user-info p {
  text-align: center;
  margin: 0px;
  margin-top: 0.2rem;
  font-size: 1.2rem;
}

.comservice-user-info .message-coupons-block, .cart-order-block {
  width: 50%;
  float: left;
}

.comservice-user-info .message-coupons-block .message-item, .coupons-item {
  float: left;
  /*background: red;*/
}

.comservice-user-info .cart-order-block .order-item, .cart-item {
  float: right;
  /*background: red;*/
}

.message-item, .coupons-item, .order-item, .cart-item {
  position: relative;
}

.comservice-badge {
  position: absolute;
  background: red;
  text-align: center;
  color: white;
  font-size: 1rem;
  /*padding: 0px;*/
}

.service {
  background: white;
  overflow: hidden;
  padding-bottom: 10px;
}

.left-service-image {
  width: 50%;
  float: left;
  border-right: 1px dashed #e5e5e5;
}

.right-service-image {
  width: 50%;
  float: left;
}

.left-service-image img {
  margin-left: 10px;
  margin-top: 10px;
  margin-right: 5px;
  display: block;
}

.right-service-image img {
  margin-left: 5px;
  margin-top: 10px;
  margin-right: 10px;
  display: block;
}



.hot-service {
  /*margin-bottom: 49px;*/
  background: white;
  /*margin-top: 10px;*/
  padding-top: 3px;
  padding-bottom: 3px;
}

.hot-service-title {
  margin-left: 10px;
  overflow: hidden;
  vertical-align: middle;
  /*border-bottom: 1px solid #e5e5e5;*/
  height: 22px;
}

.hot-service-title img {
  display: block;
  float: left;
  width: 9px;
  height: 12.5px;
  margin-top: 5px;
}

.hot-service-title span {
  font-size: 12px;
  margin-top: 3px;
  margin-left: 4px;
  color: gray;
  height: 22px;
  line-height: 22px;
}
.hot-service-buttonBox{
  width: 100%;
  overflow-x: scroll;
}
.hot-service-button {
  height:3.6rem;
  font-size: 1.2rem;
  color: #B0B0B0;
  font-weight: 600;
  border-top: 1px solid #F4F4F4;
  /*border-bottom: 1px solid #F4F4F4;*/
  display: inline-flex;
}
.hot-service-button-itemBox{
  width: 33%;
  height: 3.6rem;
  float: left;
  margin-top:0.3rem;
  text-align: center;
  border-left: 1px solid #F4F4F4;
}
.hot-service-button-item{
  width:105px;
}
.hot-service-button-itemBox:first-child{
  border-left: none;
}
.hot-service-button-item section{
  width:2.5rem;
  vertical-align: middle;
  display:inline-block ;
  line-height: 1.5rem;
}
.hot-service-button-item img{
  width:3rem;
  height:3rem;
  margin-left:0.5rem;
}
