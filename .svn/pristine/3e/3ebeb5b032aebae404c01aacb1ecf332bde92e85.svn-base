.marquee_box {
  width: 100%;
  /*float: left;*/
  display: inline-block;
  overflow: hidden;
  box-sizing: border-box;
  height: 35px;
  line-height: 35px;
  position: relative;
  cursor: pointer;
  color: rgb(213, 179, 84);
}

.marquee-sibling {
  padding: 0;
  /*width: 4%;*/
  height: 35px;
  line-height: 35px;
  float: left;
  left: 0;
  z-index: 2000;
}
