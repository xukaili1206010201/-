<script>
  var allHeight = window.screen.availHeight - $('.navigationbar').outerHeight()
    - $('.myorder-tabs').outerHeight() - $('.my-button-group').outerHeight();
  $('.all-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  $('.topay-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  $('.tosend-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  $('.tobackmoney-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  $('.alsend-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  $('.toback-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  $('.finish-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  $('.alback-msg').css({
    'height': allHeight + 'px',
    'overflow': 'scroll'
  });
  var contentH = $('.navigationbar').height() + $('.myorder-tabs').height();
  $('.order-container').css({
    'margin-top': contentH + 'px'
  });
</script>

